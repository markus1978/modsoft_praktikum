<?xml version="1.0" encoding="UTF-8"?>
<twittersearch:Model
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:twittersearch="http://www.hu-berlin.de/modsoft/TwitterSearch/Exercise4"
    xsi:schemaLocation="http://www.hu-berlin.de/modsoft/TwitterSearch/Exercise4 TwitterSearch.ecore">
  <twitter>
    <types xsi:type="twittersearch:PrimitiveType"
        name="Boolean"/>
    <types xsi:type="twittersearch:PrimitiveType"
        name="Integer"/>
    <types xsi:type="twittersearch:TwitterObjectTypeDeclaration"
        name="Tweet">
      <fields
          type="//@twitter/@types.1"
          name="retweets"/>
      <fields
          type="//@twitter/@types.1"
          name="favorits"/>
      <fields
          type="//@twitter/@types.0"
          name="hasImage"/>
      <fields
          type="//@twitter/@types.0"
          name="hasURL"/>
      <fields
          type="//@twitter/@types.3"
          name="user"/>
    </types>
    <types xsi:type="twittersearch:TwitterObjectTypeDeclaration"
        name="User">
      <fields
          type="//@twitter/@types.1"
          name="followerCount"/>
      <fields
          type="//@twitter/@types.1"
          name="friendCount"/>
    </types>
  </twitter>
  <searches
      searchType="//@twitter/@types.2">
    <condition
        xsi:type="twittersearch:IntCondition"
        operand="1000">
      <fieldExpression
          xsi:type="twittersearch:FieldReference"
          fieldDeclaration="//@twitter/@types.3/@fields.0"/>
    </condition>
    <options
        xsi:type="twittersearch:Keywords">
      <keywords>RealDonaldTrump</keywords>
    </options>
  </searches>
  <searches
      searchType="//@twitter/@types.3">
    <condition
        xsi:type="twittersearch:IntCondition"
        operand="1000">
      <fieldExpression
          xsi:type="twittersearch:FieldReference"
          fieldDeclaration="//@twitter/@types.3/@fields.0"/>
    </condition>
    <options
        xsi:type="twittersearch:Keywords">
      <keywords>RealDonaldTrump</keywords>
    </options>
  </searches>
  <searches
      searchType="//@twitter/@types.2">
    <condition
        xsi:type="twittersearch:And">
      <left
          xsi:type="twittersearch:IntCondition"
          operand="1000">
        <fieldExpression
            xsi:type="twittersearch:Dot">
          <left
              xsi:type="twittersearch:FieldReference"
              fieldDeclaration="//@twitter/@types.2/@fields.4"/>
          <right
              fieldDeclaration="//@twitter/@types.3/@fields.0"/>
        </fieldExpression>
      </left>
      <right
          xsi:type="twittersearch:Or">
        <left
            xsi:type="twittersearch:BooleanCondition">
          <fieldExpression
              xsi:type="twittersearch:FieldReference"
              fieldDeclaration="//@twitter/@types.2/@fields.3"/>
        </left>
        <right
            xsi:type="twittersearch:BooleanCondition">
          <fieldExpression
              xsi:type="twittersearch:Dot">
            <left
                xsi:type="twittersearch:FieldReference"
                fieldDeclaration="//@twitter/@types.2/@fields.0"/>
            <right
                fieldDeclaration="//@twitter/@types.2/@fields.3"/>
          </fieldExpression>
        </right>
      </right>
    </condition>
    <options
        xsi:type="twittersearch:Keywords">
      <keywords>VW</keywords>
    </options>
  </searches>
  <searches
      searchType="//@twitter/@types.2">
    <condition
        xsi:type="twittersearch:And">
      <left
          xsi:type="twittersearch:IntCondition"
          operand="1000">
        <fieldExpression
            xsi:type="twittersearch:Dot">
          <left
              xsi:type="twittersearch:FieldReference"
              fieldDeclaration="//@twitter/@types.2/@fields.4"/>
          <right
              fieldDeclaration="//@twitter/@types.3/@fields.0"/>
        </fieldExpression>
      </left>
      <right
          xsi:type="twittersearch:Or">
        <left
            xsi:type="twittersearch:BooleanCondition">
          <fieldExpression
              xsi:type="twittersearch:FieldReference"
              fieldDeclaration="//@twitter/@types.2/@fields.3"/>
        </left>
        <right
            xsi:type="twittersearch:IntCondition"
            operand="5">
          <fieldExpression
              xsi:type="twittersearch:FieldReference"
              fieldDeclaration="//@twitter/@types.2/@fields.0"/>
        </right>
      </right>
    </condition>
    <options
        xsi:type="twittersearch:Keywords">
      <keywords>VW</keywords>
    </options>
  </searches>
</twittersearch:Model>
